<% provide(:title,"Users") %>
<h1>ユーザー一覧</h1>

<table class="table table-hover" id="users-table" %>
<%=will_paginate %>
  <thead>
    <tr>
      <td>名前</td>
    </tr>
　</thead>
　
　<tbody>
　  <% @users.each do |user| %>
　  <tr>
　    <td><%=link_to user.name, user %></td>
　  </tr>
　  <% end %>
　</tbody>
</table>
<%=will_paginate %>